
d=load('C:\Users\v-yexli\Desktop\Codes\Check-in Inference\Trip Duration\cluster.txt');
d(:,3)=d(:,3)+1;
n=size(d,1);
c_id=d(:,3);
c_n=max(c_id);

cl=cell(1,c_n);

for i=1:n
    for j=1:c_n
        
        if c_id(i)==j
            cl{j}=[cl{j} i];
        end
        
    end
end
